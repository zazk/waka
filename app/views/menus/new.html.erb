<h1>Add Menu</h1>
<%= form_for :menu, url: menus_path do |f| %>
<p>
    <%= f.label :name %>  <br>
    <%= f.text_field :name %>
</p>
<p>
    <%= f.label :points %> <br>
    <%= f.number_field :points %>
</p>
<p>
    <%= f.label :description %> <br>
    <%= f.text_area :description %>
</p>
<p>
    <%= f.submit%>
</p>
<div>
<%= link_to 'Menus', menus_path %>
</div>
<% end %>